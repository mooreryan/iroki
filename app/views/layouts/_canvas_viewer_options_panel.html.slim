#options-div
  .row
    ul id="options-accordion" class="accordion" data-accordion="" data-allow-all-closed="true"
      li class="accordion-item is-active" data-accordion-item=""
        a href="#" class="accordion-title" Tree format options
        .accordion-content data-tab-content=""
          .row
            .small-12.medium-12.large-6.columns
              label for="tree-shape" Layout shape
              select id="tree-shape"
                /option id="rectangular-tree" value="rectangular-tree" Rectangular
                option id="circular-tree" value="circular-tree" Circular
                option id="radial-tree" value="radial-tree" selected="" Radial
            .small-12.medium-12.large-6.columns
              label for="tree-branch-style" Branch style?
              select id="tree-branch-style"
                option id="normalogram" value="normalogram" selected="" True length
                option id="cladogram" value="cladogram" Cladogram
          .row
            .small-12.columns
              label for="tree-sort" Sort?
              select id="tree-sort"
                option id="descending" value="descending" selected="" Sort
                option id="ascending" value="ascending" Reverse sort
                option id="not-sorted" value="not-sorted" Not sorted
          .row
            .small-12.columns
              label for="tree-rotation" Tree rotation
              input id="tree-rotation" class="slider width-100" type="range" min="0" max="355" step="5" value="0"
          /.row
          /  .small-12.columns
          /    label for="biological-root" Tree root is biologically meaningful?
          /    input id="biological-root" type="checkbox" name="biological-root" checked=""

      li class="accordion-item" data-accordion-item=""
