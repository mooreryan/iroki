              input id="size" class="slider width-100" type="range" min="5" max="50" step="5" value="20"
          .row
            .small-12.columns
              label id="padding-label" for="padding" Padding
              input id="padding" class="slider width-100" type="range" min="25" max="100" step="25" value="50"

